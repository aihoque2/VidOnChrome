{
  "manifest_version": 2,

  "name": "VidOnChrome",
  "description": "play videos from your computer to chrome. Stream onto your chromecast!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts":[
    {
        "matches": ["file://*"],
        "js": ["content.js"]
    }]
}
